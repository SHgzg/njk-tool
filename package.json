    "@types/express": "^5.0.2",
    "@types/node": "^22.15.30",
    "@types/nunjucks": "^3.2.6",
    "chokidar": "3.6.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "rollup": "^4.45.1",
    "rollup-plugin-copy": "^3.5.0",
