{
  "name": "njk-tool",
  "version": "0.0.17",
  "description": "nunjucks no JavaScript 模板工具，兼容jinja",
  "types": "dist/bundle/type.d.ts",
  "exports": {
    ".": {
      "require": "./dist/bundle/app.min.cjs",
      "import": "./dist/bundle/app.min.js"
    }
  },
  "author": "zggong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SHgzg/njk-tool.git"
  },
  "type": "module",
  "peerDependencies": {
    "nunjucks": "^3.2.4"
  },
  "files": [
